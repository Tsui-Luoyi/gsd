<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jsd.web.vehicle.po.History" table="mn_cmd_history_2">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="increment"></generator>
		</id>
		<property name="vno" type="java.lang.String" column="vno"></property>
		<property name="user_id" type="java.lang.Integer" column="user_id"></property>
		<property name="user_name" type="java.lang.String" column="user_name"></property>
		<property name="cmd_name" type="java.lang.String" column="cmd_name"></property>
		<property name="cmd_type" type="java.lang.String" column="cmd_type"></property>
		<property name="devterm_state" type="java.lang.String" column="devterm_state"></property>
		<property name="send_time" type="java.util.Date" column="send_time"></property>
		<property name="rev_time" type="java.util.Date" column="rev_time"></property>
		<property name="receive_result" type="java.lang.String" column="receive_result"></property>
	</class>
</hibernate-mapping>   
