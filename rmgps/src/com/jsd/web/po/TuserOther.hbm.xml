<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jsd.web.po.TuserOther" table="tuser_other">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="userCode" type="java.lang.String">
			<column name="user_code" />
		</property>
		<property name="password" type="java.lang.String">
			<column name="password" />
		</property>
		<property name="userName" type="java.lang.String">
			<column name="user_name" />
		</property>
		<property name="userGender" type="java.lang.String">
			<column name="user_gender" />
		</property>
		<property name="userType" type="java.lang.Integer">
			<column name="user_type" />
		</property>
		<property name="note" type="java.lang.String">
			<column name="note" />
		</property>
		<property name="tuserId" type="java.lang.Integer">
			<column name="tuser_id" />
		</property>
		<property name="uuid" type="java.lang.String">
			<column name="uuid" />
		</property>
	</class>
</hibernate-mapping>
